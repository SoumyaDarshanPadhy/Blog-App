<%- include('header') %>

    <h1>
        HOME
    </h1>
    <p>
        <%= homeText %>
    </p>

    <% posts.forEach(post => { %>
        <h1> <%= post.title %> </h1>
        <p> 
            <%= post.content.substring(0, 100)+" ..."%>
            <a href="/post/<%= post.title %>">Read More</a>
        </p>
    <% }) %>

<%- include('footer') %>